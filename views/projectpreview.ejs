        <script>
            $(document).ready(function(){
                $('[data-toggle="tooltip"]').tooltip({title: '<p style="color: #272727;">Disable this field if not required</p>', html: true, placement: "top"}); 
                $(function() {
                    $('#sortable1, #sortable2, #sortable3, #sortable4, #sortable5, #sortable6').sortable({
                    connectWith: ".connectedSortable"
                    }).disableSelection();
                 })
                $('.groupbuttons').click(function(){
                    var className='groupdropdown'+$(this).attr('id')
                    var idName=$(this).attr('id')
                  
                    if ($('.'+className).css('display')=='block'){
                        $('.'+className).css('display','none')
                        $('#'+idName).removeClass("fa-angle-up").addClass("fa-angle-down");
                    }
                    else {
                        $('.'+className).css('display','block')
                        $('#'+idName).removeClass("fa-angle-down").addClass("fa-angle-up");
                    }
                })
                $('.accordianbutton').click(function(){  
                    // var idName='#accordianicon1'
                    var idName=$(this).attr('id')
                  
                    if ($('#accordianicon'+idName).hasClass("fa-angle-up")){
                        $('#accordianicon'+idName).removeClass("fa-angle-up").addClass("fa-angle-down");
                    }
                    else {
                    
                        $('#accordianicon'+idName).removeClass("fa-angle-down").addClass("fa-angle-up");
                    }
                    if ($('#edit'+idName).css('display')=='block'){
                        $('#edit'+idName).css('display','none');
                    }
                    else {
                    
                        $('#edit'+idName).css('display','block');
                    }
                })

            });
        </script>
